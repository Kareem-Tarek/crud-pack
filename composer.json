{
  "name": "Kareem-Tarek/crud-pack",
  "description": "Reusable Laravel CRUD generator (controllers, requests, models, migrations, policies, views, routes).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "KareemTarek\\CrudPack\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KareemTarek\\CrudPack\\Providers\\CrudPackServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    { "type": "path", "url": "../crud-pack", "options": { "symlink": true } }
  ]
}
