{
  "name": "kareemtarek/crud-pack",
  "description": "A comprehensive full-stack CRUD scaffolding framework layer built around a reusable Laravel CRUD generator â†’ Controllers (web/api), FormRequests (validations), Models, Migrations, Policies (multiple authorization styles), views (blades), routes (web/api), Postman API collection (generated from API routes).",
  "type": "library",
  "license": "MIT",
  "authors": [
        {
            "name": "Kareem Tarek",
            "homepage": "https://kareemtarek.infinityfree.me/"
        }
  ],
  "keywords": [
      "laravel",
      "crud",
      "generator",
      "artisan",
      "scaffold",
      "soft-deletes",
      "bulk-actions",
      "laravel-package"
    ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "KareemTarek\\CrudPack\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KareemTarek\\CrudPack\\Providers\\CrudPackServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    { "type": "path", "url": "../crud-pack", "options": { "symlink": true } }
  ]
}
